.unity-progress-bar__container {
    min-height: 35px;
    visibility: visible;
    display: flex;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-image: url("project://database/Assets/Violet%20Theme%20Ui/Progress%20Bars/Progress%20Bar%20Background.png?fileID=1932520997363718593&guid=3884787ef484b9641b8d705dfa15ea9d&type=3#Progress Bar Background_0");
    background-color: rgba(0, 0, 0, 0);
}

.unity-progress-bar__progress {
    background-color: rgba(255, 255, 255, 0);
    background-image: url("project://database/Assets/Violet%20Theme%20Ui/Sliders/Fill%20Red.png?fileID=1100476230708988374&guid=89aff9e18247bb647979c8e71d9dc5c7&type=3#Fill Red_0");
    -unity-background-image-tint-color: rgb(255, 255, 255);
}

.unity-progress-bar__background {
    background-color: rgba(0, 0, 0, 0);
    visibility: visible;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}
